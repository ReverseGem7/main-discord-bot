{
  "name": "@typical-developers/main-discord-bot",
  "author": "typical-developers",
  "version": "2.0.0",
  "description": "The main Discord bot that is used in Typical Developers' servers.",
  "license": "GPL-3.0",
  "main": "./dist/index.js",
  "type": "module",
  "imports": {
    "#lib/*": "./dist/lib/*.js"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc",
    "build:watch": "pnpm run clean && tsc -w",
    "deploy": "pnpm run build && node ./dist/index.js",
    "deploy:dev": "set DEV_DEPLOYMENT=true&&pnpm run clean && pnpm run build && node ./dist/index.js"
  },
  "dependencies": {
    "@sapphire/discord-utilities": "^3.2.3",
    "@sapphire/discord.js-utilities": "^7.2.0",
    "@sapphire/framework": "^5.1.0",
    "@sapphire/pieces": "^4.2.2",
    "discord.js": "^14.14.1",
    "gql-query-builder": "^3.8.0",
    "graphql": "^16.8.1",
    "node-cache": "^5.1.2",
    "node-html-to-image": "^4.0.0",
    "puppeteer": "^22.7.1"
  },
  "devDependencies": {
    "@sapphire/decorators": "^6.1.0",
    "@sapphire/plugin-subcommands": "^6.0.3",
    "@types/node": "^20.12.7",
    "@typical-developers/api-types": "link:W:/Colt/Projects/typical-developers/api-types",
    "dotenv": "^16.4.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  }
}
