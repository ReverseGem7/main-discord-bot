{
  "name": "@typical-developers/main-discord-bot",
  "author": "typical-developers",
  "version": "2.0.0",
  "description": "A typical Discord bot.",
  "main": "dist/index.js",
  "type": "module",
  "imports": {
    "#lib/*": "./dist/lib/*.js"
  },
  "scripts": {
    "clean": "rimraf ./dist/",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "pnpm run clean && pnpm run format && tsc",
    "build:watch": "pnpm run clean && tsc -w",
    "deploy": "pnpm run build && node dist/index.js"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  }
}
